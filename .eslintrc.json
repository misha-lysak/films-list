{
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "extends": "airbnb-base",
  "rules": {
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "camelcase": ["error", {"properties": "never"}],
    "arrow-body-style": 0,
    "import/prefer-default-export": "off",
    "import/extensions": [
       "error",
       "ignorePackages",
       {
         "js": "never",
         "jsx": "never",
         "ts": "never",
         "tsx": "never"
       }
    ]
 }
}